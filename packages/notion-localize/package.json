{
  "name": "notion-localize",
  "version": "0.1.6",
  "description": "use Notion as localization and translation management databse",
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require":  "./dist/main.js",
      "default": "./dist/module.js"
    }
  },
  "keywords": [
    "Notion",
    "localization",
    "translation",
    "internationalization"
  ],
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build --no-source-maps",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wengcan/localize.git"
  },
  "bugs": {
    "url": "https://github.com/wengcan/localize/issues"
  },
  "homepage": "https://github.com/wengcan/localize#readme",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@parcel/packager-ts": "^2.0.1",
    "@parcel/transformer-typescript-types": "^2.0.1",
    "@types/node": "^16.11.9",
    "parcel": "^2.0.1",
    "typescript": "^4.5.2"
  },
  "targets": {
    "default": {
      "context": "node",
      "includeNodeModules": false,
      "sourceMap": false,
      "optimize": false,
      "isLibrary": true
    }
  },
  "engines": {
    "node": ">=12"
  }
}
